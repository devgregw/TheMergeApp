<UserControl x:Class="Merge_Data_Utility.UI.Controls.EditorFields.FieldHeader"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:local="clr-namespace:Merge_Data_Utility.UI.Controls.EditorFields"
             mc:Ignorable="d"
             d:DesignHeight="300" d:DesignWidth="500">
    <StackPanel Background="White">
        <TextBlock x:Name="title" Foreground="Black" TextWrapping="Wrap" FontSize="18" />
        <TextBlock x:Name="desc" Foreground="Black" TextWrapping="Wrap" />
    </StackPanel>
</UserControl>