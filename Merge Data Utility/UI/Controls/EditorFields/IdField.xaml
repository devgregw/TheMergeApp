<UserControl x:Class="Merge_Data_Utility.UI.Controls.EditorFields.IdField"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:local="clr-namespace:Merge_Data_Utility.UI.Controls.EditorFields"
             mc:Ignorable="d"
             d:DesignHeight="300" d:DesignWidth="300">
    <StackPanel HorizontalAlignment="Left" VerticalAlignment="Top" Orientation="Horizontal">
        <TextBox x:Name="box" IsReadOnly="True" MaxLength="8" Width="100" />
        <Button x:Name="regenButton" Margin="5,0,0,0" Click="RegenerateRequested">Regenerate</Button>
    </StackPanel>
</UserControl>